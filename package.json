  },
  "homepage": "https://github.com/jokoho48/language-arma-atom-continued",
  "repository": {
    "type": "git",
    "url": "https://github.com/jokoho48/language-arma-atom-continued.git"
  },
  "bugs": {
    "url": "https://github.com/jokoho48/language-arma-atom-continued/issues"
  },
